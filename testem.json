{
  "framework": "qunit",
  "test_page": "tests/index.html?hidepassed",
  "launch_in_ci": [
    "PhantomJS",
    "SL_internet_explorer_9_Windows_7",
    "SL_internet_explorer_10_Windows_7",
    "SL_internet_explorer_11_Windows_8.1",
    "SL_firefox",
    "SL_chrome"
  ],
  "launch_in_dev": [
    "PhantomJS",
    "Chrome",
    "Firefox",
    "Safari"
  ],
  "launchers": {
    "SL_chrome": {
      "exe": "./node_modules/.bin/ember-cli-sauce",
      "args": [
        "-b",
        "chrome",
        "-p",
        "Windows 8.1",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_internet_explorer_9_Windows_7": {
      "exe": "./node_modules/.bin/ember-cli-sauce",
      "args": [
        "-b",
        "internet explorer",
        "-v",
        "9",
        "-p",
        "Windows 7",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_internet_explorer_10_Windows_7": {
      "exe": "./node_modules/.bin/ember-cli-sauce",
      "args": [
        "-b",
        "internet explorer",
        "-v",
        "10",
        "-p",
        "Windows 7",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_internet_explorer_11_Windows_8.1": {
      "exe": "./node_modules/.bin/ember-cli-sauce",
      "args": [
        "-b",
        "internet explorer",
        "-v",
        "11",
        "-p",
        "Windows 8.1",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_firefox": {
      "exe": "./node_modules/.bin/ember-cli-sauce",
      "args": [
        "-b",
        "firefox",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    }
  }
}